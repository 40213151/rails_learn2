<div class="main users_new">
    <div class="container">
        <div class="form-heading">新規ユーザー登録</div>
        <div class="form users-form">
            <div class="form-body">
                
                <%= @yosakoiyer.errors.full_messages.each do |message|%>
                    <div class="form-error">
                        <%= message%>
                    </div>
                <% end%>
                <%=form_tag("/yosakoiyers/create") do%>
                <p>なまえ</p>
                    <input type="text" name="yosaname" value="<%=@yosakoiyer.yosaname%>"></input>
                <p>メアド</p>
                    <input name="mail" value="<%=@yosakoiyer.mail%>"></input>
                <p>パスワード</p>
                    <input name="password" type="password" value="<%= @yosakoiyer.password%>"></input>
                <input type="submit" value="新規登録"></input>
                <% end%>
                
                
            </div>
        </div>
    </div>
</div>